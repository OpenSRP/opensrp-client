<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff313131"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="10">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="9.5"
            android:background="@color/text_black"
            android:orientation="vertical"
            android:paddingBottom="10dp"
            android:paddingTop="10dp">

            <com.andexert.library.RippleView
                android:id="@+id/btn_zm_register"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                app:rv_centered="true"
                app:rv_rippleDuration="200">

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardPreventCornerOverlap="false">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="122dp"
                        android:background="@drawable/bg_texture"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <FrameLayout
                            android:id="@+id/zmContainer"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="10dp"
                            android:layout_weight="0.8">

                            <ImageButton
                                style="@style/ImageButton.Home.Register"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_gravity="center"
                                android:scaleType="fitXY"
                                android:src="@drawable/register_zm" />

                            <TextView
                                android:id="@+id/zm_location_filtered"
                                android:layout_width="82dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginBottom="5dp"
                                android:layout_marginLeft="35dp"
                                android:layout_marginTop="12dp"
                                android:background="@color/dull_red_dark"
                                android:gravity="center_horizontal"
                                android:text="Location Filtered"
                                android:textColor="@android:color/white"
                                android:textSize="12sp"
                                android:textStyle="bold" />

                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.4">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:text="Program Register"
                                android:textColor="@color/dull_white"
                                android:textSize="25sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.8"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="0.8"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/txt_zm_register_client_count"
                                        style="@style/Badge"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_weight="0.5"
                                        android:text="0000 M"
                                        android:textSize="13sp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/zm_register_intersex_count"
                                        style="@style/Badge"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_weight="0.5"
                                        android:text="0000 T"
                                        android:textSize="13sp"
                                        android:textStyle="bold" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/zm_register_female_count"
                                        style="@style/Badge"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_weight="0.5"
                                        android:text="0000 F"
                                        android:textSize="13sp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/zm_register_male_count"
                                        style="@style/Badge"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_weight="0.5"
                                        android:text="0000 M"
                                        android:textSize="13sp"
                                        android:textStyle="bold" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <TextView
                                        android:id="@+id/txt_zm_register_child_count"
                                        style="@style/Badge"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_weight="0.5"
                                        android:text="0000 C"
                                        android:textSize="13sp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/txt_zm_register_woman_count"
                                        style="@style/Badge"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_gravity="center_vertical"
                                        android:layout_weight="0.5"
                                        android:text="0000 W"
                                        android:textSize="13sp"
                                        android:textStyle="bold" />

                                </LinearLayout>

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>
                </android.support.v7.widget.CardView>
            </com.andexert.library.RippleView>

            <com.andexert.library.RippleView
                android:id="@+id/btn_household_register"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="8dp"
                app:rv_centered="true"
                app:rv_rippleDuration="200">

                <android.support.v7.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardPreventCornerOverlap="false">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="110dp"
                        android:background="@drawable/bg_texture"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <FrameLayout
                            android:id="@+id/householdContainer"
                            android:layout_width="0dp"
                            android:layout_height="fill_parent"
                            android:layout_marginRight="10dp"
                            android:layout_weight="0.8">

                            <ImageButton
                                style="@style/ImageButton.Home.Register"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_gravity="center"
                                android:scaleType="fitXY"
                                android:src="@drawable/register_household" />

                        </FrameLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1.4">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center_vertical"
                                android:text="Household Register"
                                android:textColor="@color/dull_white"
                                android:textSize="25sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="0.8"
                            android:orientation="horizontal"
                            android:weightSum="1">

                            <TextView
                                android:id="@+id/txt_household_register_client_count"
                                style="@style/Badge"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_weight="0.5"
                                android:text="0000M"
                                android:textSize="13sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/txt_household_register_client_plus_members_count"
                                style="@style/Badge"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_weight="0.5"
                                android:text="0000 H"
                                android:textSize="13sp"
                                android:textStyle="bold" />
                        </LinearLayout>


                    </LinearLayout>

                </android.support.v7.widget.CardView>
            </com.andexert.library.RippleView>

            <com.andexert.library.RippleView
