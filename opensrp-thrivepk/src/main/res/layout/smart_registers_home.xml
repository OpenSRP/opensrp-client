                            <ImageButton
                                style="@style/ImageButton.Home.Register"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_gravity="center"
                                android:scaleType="fitXY"
                                android:src="@drawable/register_child" />

                        </FrameLayout>

                        <LinearLayout
